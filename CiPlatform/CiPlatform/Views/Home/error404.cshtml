@{
    ViewData["Title"] = "notFound";
}

<partial name="_header.cshtml" />

<div class="container-xl text-center">

    <img class="img-fluid d-none d-md-inline-block" src="~/assets/404-Page-image.png" />
    <h4 class="mt-5 my-md-2">Page not Found</h4>
    <h1 class="mt-3 d-none d-md-block"><b>Oops!</b> The page you were looking for doesn't exist.</h1>
    <h5 class="lead d-none d-md-block">You may have misstyped the address or the page may have moved.</h5>
    <a class="btn login-btn my-4 px-4 py-2" asp-controller="Home" asp-action="home">
        Go to Home &nbsp <img src="~/assets/right-arrow.png" alt="">
    </a>
</div>

